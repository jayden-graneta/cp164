# Imports
from List_linked import List

t1 = List()
t2 = List()
for x in range(10):
    t1.append(x)
    t2.append(x)

print(t1.is_identical_r(t2))

t3 = List()

for x in range(10):
    t3.append(x + 1)

even, odd = t3.split_alt()
print(even._front._next._value, odd._front._next._value)

t4 = List()
t5 = List()
t6 = List()

for x in range(5):
    t4.append(x)
    t5.append(x)

t6.intersection(t4, t5)

for i in t6:
    print(i)
